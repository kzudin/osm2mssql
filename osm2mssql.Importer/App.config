<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=5.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
		<sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="osm2mssql.Importer.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
		</sectionGroup>
	</configSections>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
	</startup>
	<entityFramework>
		<defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework"/>
	</entityFramework>
	<connectionStrings>
		<add name="OSMEntities" providerName="System.Data.SqlClient" connectionString="DataSource=(localdb)\v11.0; Initial Catalog=OSM"/>
	</connectionStrings>
	<applicationSettings>
		<osm2mssql.Importer.Properties.Settings>
			<setting name="MaxRowCountInMemory" serializeAs="String">
				<value>10000</value>
			</setting>
			<setting name="MaxPendingOperations" serializeAs="String">
				<value>12</value>
			</setting>
			<setting name="SqlDirs" serializeAs="Xml">
				<value>
					<ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
					  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
						<string>SQL\Changes</string>
						<string>SQL\Procedures</string>
					</ArrayOfString>
				</value>
			</setting>
		</osm2mssql.Importer.Properties.Settings>
	</applicationSettings>
</configuration>